{
  "name": "sora-anniversary",
  "version": "1.0.0",
  "description": "Webpage for Sora's 3rd Anniversary",
  "scripts": {
    "minify_css": "npx cleancss -o index.min.css index.css",
    "compile_js": "npx babel index.js | uglifyjs > index.min.js",
    "compile_handlebars": "node compile_index.js > index.html",
    "compile": "npm run minify_css && npm run compile_js && npm run compile_handlebars"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Skk-tj/sora-anniversary.git"
  },
  "dependencies": {
    "@babel/cli": "^7.11.5",
    "@babel/core": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "clean-css-cli": "^4.3.0",
    "country-code-lookup": "0.0.17",
    "csv-parse": "^4.12.0",
    "handlebars": "^4.7.6",
    "html-minifier": "^4.0.0",
    "uglifyjs": "^2.4.11"
  }
}
